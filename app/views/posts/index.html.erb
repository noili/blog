<h1>Post Index</h1>
<%@posts.each do |post|%>
  <h1><%=post.title.titleize%></h1>
  <p><%=post.body%></h1></p>
  <%=link_to 'Show', post_path(post)%>
  <%=link_to 'Edit', edit_post_path(post)%>
  <%=link_to 'Delete', post_path(post),
              method: :delete,
              data: { confirm: 'Are you sure?'} %>
<%end%>
